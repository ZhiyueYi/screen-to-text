{
  "name": "screen-to-text",
  "version": "1.0.0",
  "description": "An electron mini app to help you copy texts from screenshots",
  "main": "main.js",
  "scripts": {
    "start": "npm-run-all --serial delete:dist build copy start:electron",
    "start:electron": "electron dist",
    "build": "tsc",
    "test": "jest",
    "copy": "npm-run-all --parallel copy:package copy:views",
    "copy:views": "cpx src/index.html dist",
    "copy:package": "cpx package.json dist",
    "delete:dist": "rimraf dist"
  },
  "author": "Zhiyue Yi",
  "license": "MIT",
  "devDependencies": {
    "cpx": "^1.5.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "electron": "^7.1.7",
    "tesseract.js": "^2.0.1"
  }
}
